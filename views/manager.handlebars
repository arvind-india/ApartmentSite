<body>
		<header>
			{{> managernav }}
		</header>

		<main>			
			<div class="row">
				<div class="col s12 l12">				
					{{!-- <table class="striped responsive-table"> --}}
					<table class="bordered">	
						{{#if prospectsData}}						
							<thead class="blue lighten-2">
							  <th>First Name</th>
							  <th>Last Name</th>
							  <th>Email</th>
							  <th>Telephone</th>
							  <th>Bedrooms</th>
							  <th>Move In Date</th>
							  <th class="message">Message</th>
							  <th>Created At</th>
							  <th>Updated Ad</th>
							  <th></th>
							</thead>
						  {{#each prospectsData}}						  	
							<tbody>
						    <tr id={{id}}>
						      <td><strong>{{first_name}}</strong></td>
						      <td><strong>{{last_name}}</strong></td>
						      <td>{{email}}</td>
						      <td class="telephone-number">{{telephone}}</td>
						      <td>{{bedrooms}}</td>
						      <td>{{move_in_date}}</td>
						      <td>{{message}}</td>
						      <td>{{createdAt}}</td>
						      <td>{{updatedAt}}</td>
						      <td><a class="btn-floating btn-medium waves-effect waves-light blue darken-4"><i class="material-icons">delete</i></a></td>   
						    </tr>
						  {{/each}}						  
						{{/if}}
						{{#if contractorsData}}
							<thead class="blue lighten-2">
								<th>Business Name</th>
								<th>Doing Business As</th>
								<th>Street Address</th>
								<th>City</th>
								<th>Zip Code</th>
								<th>NAICS Code</th>
								<th>Description</th>
								<th>Created At</th>
								<th>Updated At</th>
								<th></th>
							</thead>
						  {{#each contractorsData}}
						  	<tbody>
						  	<tr>
							<td>{{business_name}}</td>
							<td>{{doing_business_as_name}}</td>
							<td>{{street_address}}</td>
							<td>{{city}}</td>
							<td>{{zip_code}}</td>
							<td>{{naics}}</td>
							<td>{{primary_naics_description}}</td>
							<td>{{createdAt}}</td>
							<td>{{updatedAt}}</td>
							<td><a class="btn-floating btn-medium waves-effect waves-light blue darken-4"><i class="material-icons">mode_edit</i></a></td>
							</tr>
						   {{/each}}
						{{/if}}
						{{#if tenantsData}}
							<thead class="blue lighten-2">
								<th>Name</th>
								<th>Email</th>
								<th>Street Address</th>
								<th>Apt#</th>
								<th>City</th>
								<th>State</th>
								<th>Zip Code</th>
								<th>Rent</th>
								<th>Rent Last Paid</th>
								<th>Total Last Paid</th>
								<th>Lease Start</th>
								<th>Lease End</th>
								<th>Parking Permit</th>
								<th>Permit #</th>
								<th>Permit Status</th>
								<th></th>
							</thead>
						  {{#each tenantsData}}
						  	<tbody>
						  	<tr>
							<td>{{name}}</td>
							<td>{{email}}</td>
							<td>{{street_address}}</td>
							<td>{{apartment_number}}</td>
							<td>{{city}}</td>
							<td>{{state}}</td>
							<td>{{zip_code}}</td>
							<td>{{no_of_occupants}}</td>
							<td>{{rent}}</td>
							<td>{{rent_last_paid}}</td>
							<td>{{total_rent_paid}}</td>
							<td>{{lease_start}}</td>
							<td>{{lease_end}}</td>
							<td>{{parking_permit}}</td>
							<td>{{parking_permit_no}}</td>
							<td>{{parking_permit_status}}</td>
							<td><a class="btn-floating btn-medium waves-effect waves-light blue darken-4"><i class="material-icons">mode_edit</i></a></td>
							<td><a id={{id}} class="btn-floating btn-medium waves-effect waves-light blue darken-4"><i class="material-icons">mode_edit</i></a></td>
							</tr>
						   {{/each}}
						{{/if}}										  
						</tbody>
					</table>
				</div>
			</div>
		</main>

		</main>
		
		{{!-- Footer Partial --}}
		{{> footer }}

		<!--Import jQuery before materialize.js-->
		<script type="text/javascript" src="https://code.jquery.com/jquery-2.1.1.min.js"></script>
		<!-- Compiled and minified JavaScript -->
		<script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.98.2/js/materialize.min.js"></script>
		<script type="text/javascript" src="/javascripts/main.js"></script>      	
	</body>