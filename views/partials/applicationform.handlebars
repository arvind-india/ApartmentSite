<section>
  <div class="row">
    <form class="col s12 l6 offset-l3">
      <div class="row">
        <div class="input-field col s6">
        <input id="first_name" type="text" class="validate">
        <label for="first_name">First Name</label>
      </div>
      <div class="input-field col s6">
        <input id="last_name" type="text" class="validate">
        <label for="last_name">Last Name</label>
      </div>
      <div class="row">
        <div class="input-field col s12 l6">
          <input id="email" type="email" class="validate">
          <label for="email">Email</label>
        </div>
        <div class="input-field col s12 l6">
          <input id="email" type="email" class="validate">
          <label for="email">Phone</label>
        </div>
      </div>
      </div>
        <div class="row">
          <div class="input-field col s6">
            <input id="input_text" type="text" data-length="9">
            <label for="input_text">Social Security #</label>
          </div>
          <div class="input-field col s6">
            <input placeholder="Birthdate" type="date" class="datepicker">
          </div>
        </div>
        <div class="row">
          <div class="input-field col s6 l6">
            <input id="drivers_license" type="text">
            <label for="drivers_license">Driver's License #</label>
          </div>
          <div class="input-field col s3 l3">
            <input id="state" type="text">
            <label for="state">State</label>
          </div>
        </div>
        <div class="row">
          <div class="col s6 l6"><h5>Current Address</h5></div>
        </div>
        <div class="row">                  
          <div class="input-field col s9 l9">
            <input id="current_street" type="text">
            <label for="current_street">Street Address</label>
          </div>                  
        </div>
        <div class="row">
          <div class="input-field col s7 l7">
            <input id="current_city" type="text">
            <label for="current_city">City</label>
          </div>                  
          <div class="input-field col s2 l2">
            <input id="current_state" type="text">
            <label for="current_state">State</label>
          </div>
          <div class="input-field col s3 l3">
            <input id="current_zip" type="text">
            <label for="current_zip">Zip</label>
          </div>
        </div>
        <div class="row">
          <div class="col s6 l6"><h5>Property Information</h5></div>
        </div>
        <div class="input-field col s12 l12">
          <select>
            <option value="" disabled selected>Do you plan on having a pet?</option>
            <option value="1">Yes</option>
            <option value="2">No</option>                    
          </select>
          <div class="col s12">
            Pet(s) description:
            <div class="input-field inline">
              <input id="pet_descrip" type="text">
              <label for="pet_descrip">Description</label>
            </div>
          </div>                  
        </div>
        <div class="row">
          <div class="col s6 l6"><h5>Rental History</h5></div>
        </div>
        <div class="row">                  
          <div class="input-field col s9 l9">
            <input id="street1" type="text">
            <label for="street1">Street Address</label>
          </div>                  
        </div>
        <div class="row">
          <div class="input-field col s7 l7">
            <input id="city1" type="text">
            <label for="city1">City</label>
          </div>                  
          <div class="input-field col s2 l2">
            <input id="state1" type="text">
            <label for="state1">State</label>
          </div>
          <div class="input-field col s3 l3">
            <input id="zip1" type="text">
            <label for="zip1">Zip</label>
          </div>
        </div>
        <div class="row">
          <div class="col s6 l6"><h5>Employment History</h5></div>
        </div>
        <div class="row">
          <div class="col s6 l6"><h5>Vehicles</h5></div>
        </div>
        <button class="btn waves-effect waves-light" type="submit" name="action">Submit
          <i class="material-icons right">send</i>
        </button>                             
    </form>
  </div>
</section>